<div class="subscriptions">
  <div class="col-md-4">
    <% if @subscriptions.present? %>
      <h3>Subscriptions</h3>
    <% end %>
  </div>
  <div class="col-md-8 licence-alert alert alert-danger" style="display:none">
    <span id="licence-message">You require <span class="licence-message-7"> (7 days) ,</span><span class="licence-message-30"> (30 days) ,</span><span class="licence-message-90"> (90 days) ,</span><span class="licence-message-infinity"> (infinity) </span> additional Plans.</span>
  </div>
  <% if @subscriptions.present? %>
  <div class="col-md-12">
    <table id="subscription_data" class="table table-striped table-bordered" cellspacing="0" width="100%">
      <div id="img-subscription-loader" class="img-subscription-loader hide">
        <%= image_tag('loader3.gif', alt: 'Loading', class: 'center-gif') %>
      </div>
      <thead>
      <tr>
        <th class="col-md-2">ID</th>
        <th class="col-md-2">Plan</th>
        <th class="col-md-1">Quantity</th>
        <th class="col-md-1">Price (<i class='fa fa-eur'></i>)</th>
        <th class="col-md-2">Creation</th>
        <th class="col-md-1">Period</th>
        <th class="col-md-2">Auto-Renewal</th>
        <th class="col-md-1">Options</th>
      </tr>
      </thead>
    </table>
  </div>
  <% end %>
  <div class="col-md-12 margin-bottom-40">
    <input id="open-modal" class="btn btn-primary save-sub-button" value="Add Subscription" type="button">
  </div>
</div>
